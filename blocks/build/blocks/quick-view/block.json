{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wc-booster/quick-view",
  "version": "1.3",
  "title": "Quick View",
  "description": "This Block adds a quick view button to each product listing, allowing customers to preview products without leaving the current page.",
  "attributes": {
    "top": {
      "type": "object",
      "default": {
        "activeUnit": "px",
        "units": [
          "px",
          "em"
        ],
        "range": {
          "min": 1,
          "max": 500
        },
        "values": {
          "desktop": 45,
          "tablet": 45,
          "mobile": 45
        }
      }
    },
    "left": {
      "type": "object",
      "default": {
        "activeUnit": "px",
        "units": [
          "px",
          "em"
        ],
        "range": {
          "min": 1,
          "max": 500
        },
        "values": {
          "desktop": 22,
          "tablet": 22,
          "mobile": 22
        }
      }
    },
    "iconPosition": {
      "type": "string",
      "default": "absolute"
    },
    "textTypo": {
      "type": "object",
      "default": {
        "fontFamily": "",
        "fontSize": {
          "units": [
            "px",
            "em",
            "rem"
          ],
          "activeUnit": "px",
          "values": {
            "desktop": 15,
            "tablet": 15,
            "mobile": 15
          }
        },
        "fontWeight": 500,
        "lineHeight": {
          "activeUnit": "px",
          "units": [
            "px"
          ],
          "values": {
            "desktop": 28,
            "tablet": 28,
            "mobile": 28
          }
        }
      }
    },
    "color": {
      "type": "string",
      "default": "#515151"
    },
    "bgColor": {
      "type": "string",
      "default": ""
    },
    "enableText": {
      "type": "boolean",
      "default": false
    },
    "title": {
      "type": "string",
      "default": "Quick View"
    },
    "enableTextOnHover": {
      "type": "string",
      "default": "visible"
    },
    "textPosition": {
      "type": "string",
      "default": "right"
    }
  },
  "usesContext": [
    "postId"
  ],
  "textdomain": "wc-booster",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}